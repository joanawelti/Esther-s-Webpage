			<label index="common_regularText_description">Elément texte avec titre et paragraphe</label>
			<label index="common_textImage_title">Texte avec image</label>
			<label index="common_textImage_description">Images aligné à droite d'un élément de texte.</label>
			<label index="common_imagesOnly_title">Images seules</label>
			<label index="common_imagesOnly_description">Images alignées en colonnes, avec une légende</label>
			<label index="common_bulletList_title">Liste à puce</label>
			<label index="common_bulletList_description">Un liste à puce simple</label>
			<label index="common_table_title">Tableau</label>
			<label index="common_table_description">Un tableau simple avec un maximum de 8 colonnes</label>
			<label index="special_filelinks_title">Liens vers des fichiers</label>
			<label index="special_filelinks_description">Créer une liste de fichier pour téléchargement.</label>
